@InBook{Schmidt2014,
  pages     = {427--444},
  title     = {The Concept of `Practice': What's the Point?},
  publisher = {Springer International Publishing},
  year      = {2014},
  author    = {Schmidt, Kjeld},
  editor    = {Rossitto, Chiara and Ciolfi, Luigina and Martin, David and Conein, Bernard},
  address   = {Cham},
  isbn      = {978-3-319-06498-7},
  abstract  = {The purpose of the present article is to argue that the point of using the concept of `practice' in the context of CSCW and related research areas is to overcome the categorial separation of `thinking' and `acting' that is part and parcel of the received discourse about `work' and thus to be able to focus sharply on and express the unity of the activities of work. The article shows that received concept of `practice' has been developed in the course of centuries precisely as a way to focus on normatively regulated contingent activities.},
  booktitle = {COOP 2014 - Proceedings of the 11th International Conference on the Design of Cooperative Systems, 27-30 May 2014, Nice (France)},
  doi       = {10.1007/978-3-319-06498-7_26},
  file      = {Schmidt2014.pdf:diss-ethnographie/Schmidt2014.pdf:PDF},
  groups    = {diss-methode},
